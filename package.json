{
  "name": "@map3xyz/assets-helper",
  "version": "1.0.182",
  "description": "A library for maintaining the assets repo.",
  "author": "pellicceama",
  "keywords": [
    "typescript",
    "starter"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "/dist/index.d.ts",
  "bin": "dist/index.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "index": "ts-node ./src/index.ts",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "yarn clean && tsc",
    "preparePublish": "yarn build && npm version patch",
    "prepareTest": "yarn ts-node ./src/cache.ts",
    "test": "yarn prepareTest && yarn build && ava",
    "bundle": "yarn build && pkg . --out-dir ./exec/",
    "release-db": "ts-node ./src/releaser/sync/sync-database.ts"
  },
  "devDependencies": {
    "@types/ethereum-checksum-address": "^0.0.0",
    "@types/node": "^18.0.0",
    "ava": "^4.3.0",
    "pkg": "^5.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@octokit/auth-token": "^3.0.0",
    "@octokit/rest": "^19.0.3",
    "@types/moment": "^2.13.0",
    "@types/sqlite3": "^3.1.8",
    "@uniswap/token-lists": "^1.0.0-beta.30",
    "algoliasearch": "^4.15.0",
    "axios": "^0.27.2",
    "csv-parse": "^5.3.0",
    "dotenv": "^16.0.1",
    "ethereum-checksum-address": "^0.0.8",
    "ethers": "^5.7.2",
    "jsonschema": "^1.4.1",
    "ksuid": "^3.0.0",
    "moment": "^2.29.4",
    "octokit": "^2.0.5",
    "promised-sqlite3": "^1.2.0",
    "shelljs": "^0.8.5",
    "sqlite3": "^5.0.11",
    "uuid": "^8.3.2"
  },
  "packageManager": "yarn@3.2.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/map3xyz/assets-helper.git"
  },
  "bugs": {
    "url": "https://github.com/map3xyz/assets-helper/issues"
  },
  "homepage": "https://github.com/map3xyz/assets-helper#readme"
}
